    "import cv2\n",
    "import numpy\n",
    "img = cv2.imread('img/lena.png')\n",
    "LAB = cv2.cvtColor(img , cv2.COLOR_BGR2LAB)\n",
    "HLS = cv2.cvtColor(img , cv2.COLOR_BGR2HLS)\n",
    "\n",
    "cv2.imshow('LAB' , LAB)\n",
    "cv2.imshow('HSL' , HLS)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
